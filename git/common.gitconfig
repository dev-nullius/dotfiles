[core]
  editor = vim
[user]
  name = "Cifra Obscura"
  email = "669802+obcifra@users.noreply.github.com"
  signingKey = "A38884F84FE144F4F98E4C45F4A51BDD51ADB56D"
[gpg]
  program = qubes-gpg-client-wrapper
[commit]
  gpgSign = true
[alias]
  lg = log --graph --oneline --decorate --all
[include]
  path = ~/.gitconfig-local
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = auto
[push]
  default = nothing
[credential]
  helper = cache --timeout=28800
[pull]
  rebase = true
[fetch]
  prune = true
[diff]
  tool = vimdiff
  colorMoved = zebra
[merge]
  tool = vimdiff
